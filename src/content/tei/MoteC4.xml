	<text>
		<body>
			<head>
				<title>/Ciiii/</title>
			</head>
			<lg n="1" type="poema" corresp="dama" met="3x7" rhyme="abb" >
				<l n="1">Buscaréis por estas damas </l> 
				<l n="2">y, si, ay Gerónima!, </l>
				<l n="3">dezilde cómo le va.</l>
			</lg>

			<lg n="2" type="poema" corresp="caballero" met="4x7" rhyme="aabba" >
				<l n="0">Gerónima </l>
				<l n="1">dezirme ha, va como va, </l>
				<l n="2">yo diré, no como deve,</l>
				<l n="3">pues que sé qu’ ella no beve </l>
				<l n="4">con el cáliz que me da.</l>
			</lg> 
		</body>
